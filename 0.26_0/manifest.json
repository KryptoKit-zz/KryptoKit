{
   "author": "Steven Dakh",
   "background": {
      "scripts": [ "jquery.min.js", "openpgp.min.js", "bitcoinjs-min.js", "crypto.js", "tx.js", "util.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "KryptoKit_128.png",
      "default_popup": "popup.html"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+K"
         }
      }
   },
   "description": "In-browser Bitcoin Wallet and GPG tools",
   "icons": {
      "128": "KryptoKit_128.png",
      "16": "KryptoKit_16.png",
      "48": "KryptoKit_48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "16.0.884",
   "name": "KryptoKit",
   "permissions": [ "clipboardWrite", "storage", "\u003Call_urls\u003E" ],
   "short_name": "KryptoKit",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.26"
}
